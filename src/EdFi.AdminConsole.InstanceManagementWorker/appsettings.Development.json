{
    "AppSettings": {
        "DatabaseEngine": "Postgresql",
        "PathBase": "",
        "DefaultPageSizeOffset": 0,
        "DefaultPageSizeLimit": 25,
        "OverrideExistingDatabase": false,
        "IgnoresCertificateErrors": true
    },
    "AdminApiSettings": {
        "AdminConsoleTenantsURI": "https://localhost/adminapi/adminconsole/tenants",
        "AdminConsoleInstancesURI": "https://localhost/adminapi/adminconsole/instances?status={0}",
        "AdminConsoleCompleteInstancesURI": "https://localhost/adminapi/adminconsole/instances/{InstanceId}/completed",
        "AccessTokenUrl": "https://localhost/auth/realms/myrealm/protocol/openid-connect/token",
        "Username": "myuser",
        "ClientId": "ac",
        "Password": "SomePassword"
    },
    "ConnectionStrings": {
        "EdFi_Master": "host=localhost;port=5432;username=postgres;password=admin;database=postgres;pooling=false",
        "EdFi_Ods": "host=localhost;port=5432;username=postgres;password=admin;database={0};pooling=false"
    },
    "DatabaseProvider": "Npgsql",
    "connectionStringsProvider": "Npgsql",
    "Log4NetCore": {
        "Log4NetConfigFileName": "log4net\\log4net.config"
    },
    "Logging": {
        "LogLevel": {
            "Default": "Information",
            "Microsoft": "Warning",
            "OpenIddict.*": "Warning",
            "Microsoft.Hosting.Lifetime": "Information"
        }
    }
}
